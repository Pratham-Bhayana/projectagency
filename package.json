{
  "name": "bureau-engine",
  "version": "1.0.0",
  "description": "Modern web application for Bureau Engine - Engineering Digital Success",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "react",
    "nodejs",
    "express",
    "tailwindcss",
    "vite",
    "digital-agency",
    "web-development"
  ],
  "author": "Bureau Engine",
  "license": "MIT"
}
